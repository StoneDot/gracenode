{
	"name": "gracenode2",
	"description": "Framework for node.js to boostrap other node.js modules for easy use and uniformed synchronus setup.",
	"version": "2.0.0",
	"author": "Nobuyori Takahashi <voltrue2@yahoo.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/voltrue2/gracenode.git"
	},
	"scripts": {
		"preinstall": "rm -f ../../gracenode",
		"install": "ln -fs node_modules/gracenode2/aeterno ../../gracenode"
	},
	"dependencies": {
		"async": "1.4.2",
		"jshint": "2.8.0",
		"gracelog": "0.5.4",
		"aeterno": "0.5.4",
		"cluster-mode": "0.1.3",
		"node-datetime": "0.1.5"
	},
	"devDependencies": {
		"mocha": "2.3.3",
		"staticdata": "0.0.7",
		"gracenode-server": "0.4.3",
		"request": "2.64.0",
		"express": "4.13.3"
	},
	"engine": {
		"engine": "node >= 0.10.20"
	},
	"jshintConfig": {
                "node": true,
                "bitwise": false,
                "camelcase": true,
                "curly": true,
                "eqeqeq": true,
                "forin": false,
                "immed": true,
                "latedef": false,
                "newcap": true,
                "noarg": true,
                "noempty": true,
                "undef": true,
                "unused": true,
                "nonew": true,
                "white": true,
                "maxdepth": 5,
                "quotmark": "single",
                "globals": {
                        "mocha": false,
                        "describe": false,
                        "it": false,
                        "before": false,
                        "beforeEach": false,
                        "after": false,
                        "afterEach": false
                }
        },
	"license": "MIT"
}
