{
	"testConfig": {
		"blah": [
			"One",
			"Two",
			"Three"
		],
		"bool": false
	},
	"debugMode": {
		"directories": [ 
			"node_modules/gracenode/test/",
			"node_modules/gracenode/index.js"
		],
		"lintOptions": {
			"bitwise": false,
			"eqeqeq":  true,
			"latedef": true,
			"noempty": true,
			"undef":   true,
			"node":     true,
			"unused":   true,
			"trailing": true,
			"white":    true,
			"globals"   : {
				"mocha"      : false,
				"describe"   : false,
				"it"         : false,
				"before"     : false,
				"beforeEach" : false,
				"after"      : false,
				"afterEach"  : false
			}
		}
	},
	"modules": {
		"log": {
			"bufferSize": 0,
			"depth": 10,
			"console": true,
			"color": true,
			"level": ">= verbose"
		},
		"test": {

		},
		"test-driver": {
			"driverTest": true
		},
		"redis": {
			"clients": {
				"test1": {
					"host": "localhost",
					"port": 6379,
					"options": null
				}
			}
		},
		"gracenode-iap": {
			"sandbox": true,
			"sql": "dummy"
		},
		"gracenode-mysql": {
			"dummy": {
				"read": {
					"database": "dummy_read",
					"host": "localhost",
					"user": "dummy_user",
					"password": "dummy_pass"
				},
				"write": {
					"database": "dummy_write",
					"host": "localhost",
					"user": "dummy_user",
					"password": "dummy_pass"
				}
			}
		},
		"gracenode-staticdata": {
			"path": "gracenode/test/staticdata/",
			"delimiter": ","
		},
		"gracenode-cron": {
			"test": {
				"schedule": "*/30 * * * * *"
			}
		},
		"gracenode-server": {
			"protocol": "http",
			"host": "localhost",
			"port": 8000,
			"controllerPath": "gracenode/test/server/controller/"
		},
		"gracenode-udp": {
			"servers": [
				{ "name": "test", "host": "localhost", "port": 8999 }
			],
			"requests": {
				"test": { "host": "localhost", "port": 8999 }
			}
		},
		"gracenode-session": {
			"ttl":  1000
		},
		"gracenode-wallet": {
			"names": ["dummy"],
			"sql": "dummy"
		},
		"gracenode-memcache": {
			"hosts": ["localhost"],
			"ttl": 10000,
			"options": null
		},
		"gracenode-view": {

		}
	}
}
